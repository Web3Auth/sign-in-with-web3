    "@solana/web3.js": "^1.36.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "sweetalert2": "^11.4.12"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.5.0",
